<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.sharedtable.controller.ClientPropertyWindowController"
      style="-fx-padding: 20px; -fx-spacing: 10px" prefWidth="500" prefHeight="300">

    <Label></Label>
    <HBox spacing="5">
        <Label style="-fx-font-weight: bold">Egyedi azonosító: </Label>
        <Label fx:id="idLabel"></Label>
    </HBox>
    <HBox spacing="5">
        <Label style="-fx-font-weight: bold">Felhasználói név: </Label>
        <Label fx:id="nicknameLabel"></Label>
    </HBox>
    <HBox spacing="5">
        <Label style="-fx-font-weight: bold">Külső IP-cím: </Label>
        <Label fx:id="IPLabel"></Label>
    </HBox>
    <HBox spacing="5">
        <Label style="-fx-font-weight: bold">Port: </Label>
        <Label fx:id="portLabel"></Label>
    </HBox>
    <HBox spacing="5">
        <Label style="-fx-font-weight: bold">Szülő azonosító: </Label>
        <Label fx:id="parentIDLabel"></Label>
    </HBox>
    <HBox spacing="5">
        <Label style="-fx-font-weight: bold">Átlagos válaszidő: </Label>
        <Label fx:id="pingLabel"></Label>
    </HBox>
    <Button fx:id="pingButton" text="Kliens pingelése" onAction="#btnPingButtonClicked"></Button>
</VBox>


