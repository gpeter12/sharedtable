<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefHeight="100.0" prefWidth="350.0" alignment="center"
      fx:controller="com.sharedtable.controller.RenameTabController" spacing="15">
    <HBox alignment="center" spacing="15" onKeyPressed="#onKeyPressed">
        <Label>Új lap neve: </Label>
        <TextField fx:id="tabNameTextField" minWidth="100" minHeight="20" promptText="lap név"></TextField>
    </HBox>
    <Button onAction="#btnCreateNewTabClicked" text="OK"></Button>
</VBox>
